{
  "extends": ["react-app", "prettier", "airbnb"],
  "plugins": ["prettier"],
  "rules": {
    "linebreak-style": 0,
    "react/jsx-filename-extension": 0,
    "react/jsx-one-expression-per-line": 0,
    "prettier/prettier": [
      "error",
      {
          "endOfLine": "auto"
      }
    ],
    "import/no-unresolved": "off"
  }
  
}
